{
  "name": "npm-proy",
  "version": "0.0.1",
  "description": "Proy CssFlexBox",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pug": "pug -w -P -o dist/ -p src/pug/partials/ -O src/pug/options.json src/pug/templates/",
    "watch:pug": "onchange src/pug/templates/*.pug -- npm run pug",
    "lint-sass": "sass-lint \"**/*.sass\" -v -q",
    "css": "node-sass -o dist/css src/scss --source-map-embed",
    "build:css": "npm run css && npm run autoprefixer",
    "watch:css": "onchange src/scss/*.scss -- npm run build:css",
    "babel": "babel src/es6/ -d src/js/",
    "build:babel": "npm run babel",
    "watch:babel": "onchange src/es6/ -- npm run babel",
    "js": "browserify src/js/index.js > dist/js/bundle.js",
    "watch:js": "onchange src/js/index.js -- npm run js",
    "statics": "node statics",
    "imagemin": "imagemin \"src/img/*.+(jpg|png)\" -o dist/img/ -p=pngquant -p=jpeg-recompress",
    "webp": "imagemin \"dist/img/*.jpg\" -o dist/img/ -p=webp",
    "svgmin": "imagemin \"src/img/*.svg\" -o dist/img/ -p=svgo",
    "gifmin": "imagemin \"src/img/*.gif\" -o dist/img/ -p=gifsicle",
    "clean": "rimraf dist/*",
    "build:folders": "mkdirp dist/css dist/js dist/img",
    "autoprefixer": "postcss --use autoprefixer --autoprefixer.browsers '> 5%, ie 9' -r dist/css/* ",
    "uncss": "uncss -u uncss.json \"src/*.+(html|php)\" > dist/css/style.css",
    "min:css-js": "node buildify",
    "useref": "useref \"src/**/*.+(html|php)\" dist",
    "htmlmin": "html-minifier --html5 --remove-comments --collapse-whitespace --input-dir dist/ --output-dir dist/",
    "dev": "parallelshell \"npm run watch:js\" \"npm run watch:css\" \"npm run watch:pug\" \"npm run watch:babel\" \"npm run serv -s\"",
    "build:media": "npm run imagemin && npm run webp && npm run svgmin && npm run gifmin",
    "serv": "browser-sync start --server dist --files dist"
  },
  "pre-commit": [
  ],
  "repository": {
  },
  "keywords": [
      "frontend",
    "scroll"    
  ],
  "author": "Ariel Triana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atrodriguez88/scrolling"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "browser-sync": "^2.18.8",
    "buildify": "^0.4.0",
    "eslint": "^3.18.0",
    "html-minifier": "^3.4.2",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.1",
    "imagemin-webp": "^4.0.0",
    "jquery": "^3.2.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.1",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^3.0.0",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "uncss": "^0.14.1",
    "useref": "^1.2.0"
  }
}
